{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": " npx eslint App.tsx && npx eslint ./src/**/* --no-error-on-unmatched-pattern && npx typescript -p ./tsconfig.json",
    "jest": "node_modules/.bin/jest",
    "jest:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test": "npm run lint && npm run jest"
  },
  "jest": {
    "preset": "jest-expo-enzyme",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@types/react-navigation": "^3.4.0",
    "expo": "^39.0.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-mixin": "^5.0.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-image-slider": "^2.0.3",
    "react-native-photo-view": "^1.5.2",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-web": "~0.13.7",
    "react-navigation": "^4.3.1",
    "react-navigation-drawer": "^2.4.6",
    "react-navigation-stack": "^2.10.1",
    "react-timer-mixin": "^0.13.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^3.6.3",
    "eslint-plugin-react-native": "^3.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-expo": "^39.0.0",
    "jest-expo-enzyme": "^1.0.0",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.1",
    "typescript": "~3.9.2"
  },
  "private": true
}
